<p id="notice"><%= notice %></p>

<p>
  <strong>Rent property name:</strong>
  <%= @rental.rent_property_name %>
</p>

<p>
  <strong>Rent:</strong>
  <%= @rental.rent.to_s(:delimited) %>円
</p>

<p>
  <strong>Address:</strong>
  <%= @rental.address %>
</p>

<p>
  <strong>Building age:</strong>
  <%= @rental.building_age %>年
</p>

<p>
  <strong>Note:</strong>
  <%= @rental.note %>
</p>

<dl>
  <% @rental.stations.each do |station| %>
    <dt>最寄り駅<%= station.id %></dt>
    <dd><strong>路線名:</strong><%= station.route_name %></dd>
    <dd><strong>駅名:</strong><%= station.station_name %></dd>
    <dd><strong>徒歩分数:</strong><%= station.walk_fraction %>分</dd>
  <% end %>
</dl>

<%= link_to 'Edit', edit_rental_path(@rental) %> |
<%= link_to 'Back', rentals_path %>
